<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>dengluServlet</servlet-name>
    <servlet-class>lihh.dengluServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>zhuceServlet</servlet-name>
    <servlet-class>lihh.zhuceServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>houseServelt</servlet-name>
    <servlet-class>lihh.houseServelt</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>dengluServlet</servlet-name>
    <url-pattern>/servlet/dengluServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>zhuceServlet</servlet-name>
    <url-pattern>/servlet/zhuceServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>houseServelt</servlet-name>
    <url-pattern>/servlet/houseServelt</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
